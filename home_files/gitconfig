[color]
  ui = auto

[color "diff"]
  meta = yellow
  frag = magenta
  old = red
  new = green

[color "status"]
  added = green
  changed = yellow
  untracked = cyan

[alias]
  lg = log --graph --decorate --pretty=oneline --abbrev-commit
  lga = log --graph --decorate --pretty=oneline --abbrev-commit --all
