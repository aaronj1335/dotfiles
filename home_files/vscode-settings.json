{
  "breadcrumbs.enabled": false,
  "cSpell.userWords": [
    "Ashly"
  ],
  "csv-preview.numberFormat": "g4",
  "debug.onTaskErrors": "showErrors",
  "editor.bracketPairColorization.enabled": false,
  "editor.fontFamily": "'JetBrainsMono Nerd Font', 'JetBrainsMono Nerd Font Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.guides.indentation": false,
  "editor.lineNumbers": "off",
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.renderWhitespace": "none",
  "editor.showFoldingControls": "never",
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "files.autoSave": "afterDelay",
  "files.exclude": {
    "**/__pycache__": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.enabled": false,
  "jupyter.askForKernelRestart": false,
  "nextcode.build.foreground": "onError",
  "notebook.showFoldingControls": "never",
  "outline.showVariables": false,
  "python.analysis.typeCheckingMode": "standard",
  "scm.inputFontSize": 16,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.smoothScrolling": true,
  "testing.automaticallyOpenPeekView": "never",
  "trailing-spaces.highlightCurrentLine": false,
  "vim.enableNeovim": true,
  "vim.hlsearch": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "U"
      ],
      "after": [
        "<c-r>"
      ]
    }
  ],
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "never",
  "window.density.editorTabHeight": "compact",
  "workbench.activityBar.location": "hidden",
  "workbench.colorTheme": "Dainty â€“ Nord (chroma 1, lightness 1)",
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": "multiple",
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": false,
  "[beancount]": {
    "outline.showConstants": false
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.rulers": [
      88
    ],
    "editor.formatOnSave": false,
    "editor.formatOnType": true
  },
  "[html]": {
    "editor.formatOnSave": false
  }
}

